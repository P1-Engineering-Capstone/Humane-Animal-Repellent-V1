<script language="JavaScript" type="text/javascript">
	$(document).ready(function() {
		$(function(){
			$('ul.sf-menu').superfish();
			// initialise supersubs before it gets hidden by tabs or superfish. Need to measure widths while visible.
			$.fn.superfish.defaults.autoArrows = false;
			$('ul.sf-menu').supersubs({
				minWidth:    19,
				maxWidth:    35,
				extraWidth:  1
			}).superfish();
		});
		$("#searchSite").focus(function() {
			if( this.value == this.defaultValue ) {
				this.value = "";
			}
		}).blur(function() {
			if( !this.value.length ) {
			this.value = this.defaultValue;
			}
		});
		// open external links in new windowws
		$("a[href^='http://'],a[href^='https://']").click(function(){
			var myHref=$(this).attr('href');
			var goodDomain="http://www.erasmusprijs.org";
			if(myHref.toLowerCase().substr(0,goodDomain.length)==goodDomain)
				return true;
			window.open(myHref);
			return false;
		});
		// open video in modal window
		$(".modalVideo").colorbox({iframe:true, innerWidth:650, innerHeight:380, opacity:.5 });
	});
